{
  "env": {
    "es6": true,
      "node": true,
      "mocha": true
  },
    "extends": [ "eslint:recommended", "eslint-config-hirsch" ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
      },
      "sourceType": "module"
    },
    "plugins": [
      "babel"
    ]
}
